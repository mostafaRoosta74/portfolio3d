{
	"name": "portfolio3d",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@nextui-org/react": "^2.3.6",
		"@react-three/drei": "^9.105.4",
		"@react-three/fiber": "^8.16.1",
		"@react-three/postprocessing": "^2.16.2",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^13.0.0",
		"@testing-library/user-event": "^13.2.1",
		"@types/jest": "^27.0.1",
		"@types/node": "^16.7.13",
		"@types/react": "^18.0.0",
		"@types/react-dom": "^18.0.0",
		"@types/wouter": "^2.2.0",
		"framer-motion": "^11.2.0",
		"leva": "^0.9.35",
		"postprocessing": "^6.35.3",
		"r3f-perf": "^7.2.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-scripts": "5.0.1",
		"three": "^0.163.0",
		"web-vitals": "^2.1.0",
		"workbox-precaching": "^7.1.0",
		"workbox-window": "^7.1.0",
		"wouter": "^3.1.3",
		"zustand": "^4.5.2"
	},
	"scripts": {
		"start": "react-scripts start",
		"build-css": "npx tailwindcss -i ./src/index.css -o ./public/output.css --watch",
		"build": "CI=false TSC_COMPILE_ON_ERROR=false react-scripts build && workbox injectManifest workbox-config.js && npm run clean-cra-sw",
		"clean-cra-sw": "rm -f build/precache-manifest.*.js && rm -f build/service-worker.js",
		"start-sw-build": "react-scripts build && workbox injectManifest workbox-config.js && npm run clean-cra-sw && serve -s build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json|tsx|jsx)\"",
		"lint": "eslint . --fix --max-warnings 0 --report-unused-disable-directives",
		"prepare": "husky install"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@eslint/js": "^9.3.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"husky": "^9.0.11",
		"prettier": "^3.2.5",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.5",
		"typescript-eslint": "^7.9.0"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	}
}
